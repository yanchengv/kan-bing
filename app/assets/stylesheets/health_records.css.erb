#zsgun {
    background: #dce0e3;
    position: relative;
    width: 292px;
    height: 900px;
    margin:10px auto;
}

a.hScrollPane_leftarrow {
    top: 0px;
    cursor: pointer;
    display: block;
    width: 292px;
    height: 10px;
    background: url(<%=asset_path "sprite.gif" %>) no-repeat top center #DBDEE2;
    left: 0;
    margin-top:-10px;
    position: absolute;
}

a.hScrollPane_leftarrow:hover {
    background: url(<%=asset_path "zsleftbar.gif" %>) no-repeat top center #DBDEE2;
}

a.hScrollPane_rightarrow {
    bottom: 0px;
    cursor: pointer;
    display: block;
    width: 292px;
    height: 10px;
    background: url(<%=asset_path "sprite.gif" %>) no-repeat bottom center #DBDEE2;
    left: 0;
    margin-bottom:-10px;
    position: absolute;
}

a.hScrollPane_rightarrow:hover {
    background: url(<%=asset_path "zsrightbar.gif" %>) no-repeat bottom center #DBDEE2;
}

.hScrollPane_dragbar {
    height: 900px;
    position: absolute;
    border-left: #d1d5d8 1px solid;
    left: 291px;
    top: 0px;
    z-index: 1;
    padding: 39px 0;
}

.hScrollPane_draghandle {
    position: absolute;
    width: 15px;
    height: 30px;
    overflow: hidden;
    background: url(<%=asset_path "gunbar.png" %>) no-repeat left top;
    _background: url(<%=asset_path "gunbar.gif" %>) no-repeat left top;
    top: 0px;
    cursor: pointer;
    left: -7px;
}

#gundiv {
    height: 100%;
    position: relative;
    margin-left: 0px;
    padding-top: 22px;
    float: left;
    display: inline;
    width: 292px;
    overflow: hidden;
}

#gundiv ul {
    width: 292px;
    position: absolute;
    height: 10000px;
}

#gundiv li {
    width: 292px;
    min-height: 90px;
    overflow: hidden;
    margin-bottom: 0px;
    position: relative;
    border-bottom:1px solid #cbcbcb;
    background:#fff;
    padding:15px;
}
#gundiv li:hover {
    width: 292px;
    min-height: 90px;
    overflow: hidden;
    margin-bottom: 0px;
    position: relative;
    border-bottom:1px solid #cbcbcb;
    border-left:3px solid #3abaeb;
    background:#fff;
    padding:15px;
}

