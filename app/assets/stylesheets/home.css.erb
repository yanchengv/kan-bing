/*------------------------------------header-------------------------------------------*/
.bg_img{
    background-color:#f4f4f4;
}
.header-nav li{
    float:left;
    list-style:none;
}
.header-nav a,.font-color{
    color:#ffffff;
}
.header-nav  li  a,#health_1,#health_2,#healthRecord,#health_4,#health_5,#health_6,#health_7,#mydoctor{
    background:transparent;
    font-size:16px;
    height:48px;
    font-family:Arial,"microsoft yahei";
    font-weight:bold;
    padding:15px 10px 10px 10px;
    width:100%;
    color:#ffffff;
    display:inline-block;
    text-shadow:1px 1px 1px rgba(0,0,0,.5);
    border-left:1px solid transparent;
    border-right:1px solid transparent;

}
header .nav li a:hover,#health_1:hover,#health_2:hover,#healthRecord:hover,#health_4:hover,#health_5:hover,#health_6:hover,#health_7:hover{
    border-top:1px solid #6ce0e3;
    border-left:1px solid #25c5c9;
    border-right:1px solid #25c5c9;
    filter:progid:DXImageTransform.Microsoft.Gradient(gradientType=0,startColorStr=#167481,endColorStr=#21b3ba);  /* IE 6 7 8  */
    background: -ms-linear-gradient(top,#167481,#21b3ba);       /* IE 10 */
    background: -moz-linear-gradient(top,#167481,#21b3ba);        /* Firefox 3.6+ */
    background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#167481),to(#21b3ba)); /* Safari 4-5, Chrome 1-9 */
    background: -webkit-linear-gradient(top,#167481,#21b3ba);   /*Safari5.1 Chrome10+*/
    background: -o-linear-gradient(top,#167481,#21b3ba);  /* Opera 11.10+*/

}
header .navbar-default{
    /*border-bottom:1px solid #00847d;*/
    /*border-top:1px solid #009892;*/
    /*border-left:1px solid #019892;*/
    /*border-right:1px solid #019892;*/
    border-bottom:1px solid #00847d;
    border-top:1px solid #22bdc1;
    border-left:1px solid #019892;
    border-right:1px solid #019892;
}
header .container > .navbar-header{
    margin:0px;
}
header .container > .navbar-collapse{
    margin:0px;
}


.navbar-right .font-yahei,.font-yahei{
    font-family: "Arial","Microsoft YaHei";
}
.font-kaiti{
    font-family:Arial,"楷体";
}
.bg{
    border-radius: 4px;
    border-top:1px solid #dae7ef;
    filter:progid:DXImageTransform.Microsoft.Gradient(gradientType=0,startColorStr=#e4edf3,endColorStr=#ffffff);  /* IE 6 7 8  */
    background: -ms-linear-gradient(top,#e4edf3,#ffffff);       /* IE 10 */
    background: -moz-linear-gradient(top,#e4edf3,#ffffff);        /* Firefox 3.6+ */
    background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#e4edf3),to(#ffffff)); /* Safari 4-5, Chrome 1-9 */
    background: -webkit-linear-gradient(top,#e4edf3,#ffffff);   /*Safari5.1 Chrome10+*/
    background: -o-linear-gradient(top,#e4edf3,#ffffff);  /* Opera 11.10+*/
}
header  .nav-bgcolor,.nav-bgcolor{
    border-radius:0px;
    box-shadow: inset 0px 0px 1px #9edad6;
    filter:progid:DXImageTransform.Microsoft.Gradient(gradientType=0,startColorStr=#27cdd1,endColorStr=#1fb0b2);  /* IE 6 7 8  */
    background: -ms-linear-gradient(top,#27cdd1,#1fb0b2);       /* IE 10 */
    background: -moz-linear-gradient(top,#27cdd1,#1fb0b2);        /* Firefox 3.6+ */
    background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#27cdd1),to(#1fb0b2)); /* Safari 4-5, Chrome 1-9 */
    background: -webkit-linear-gradient(top,#27cdd1,#1fb0b2);   /*Safari5.1 Chrome10+*/
    background: -o-linear-gradient(top,#27cdd1,#1fb0b2);  /* Opera 11.10+*/
}
header .navbar-default .navbar-toggle:hover, .navbar-default .navbar-toggle:focus {
    background-color: #fff;
}
.search-querys,.search input[type='text']{
    background: url(icon-search.png) no-repeat 3px center #ffffff;
    border:1px solid #c6c6c6;
    border-radius:4px;
    margin-top:0px;
    padding-left:18px;
}
.search-querys:focus{
    box-shadow: 0px 0px 2px #0088e3;
    outline:none;
}

.set{
    display:inline-block;
    padding-left:25px;
    width:100px;
    font-size:14px;
    line-height:30px;
    border-bottom:1px solid #efefef;
}
.icon-setting{
    background:url(<%= asset_path 'setting.png'%>) no-repeat left center;
}
.icon-out{
    background:url(<%=asset_path 'out.png' %>) no-repeat left center;
}
#_user_language{
    display: block;
    width: 100%;
    height: 34px;
    padding: 6px 12px;
    font-size: 14px;
    line-height: 1.428571429;
    color: #555555;
    vertical-align: middle;
    background-color: white;
    background-image: none;
    border: 1px solid #cccccc;
    border-radius: 4px;
    -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
    -webkit-transition: border-color ease-in-out 0.15s, box-shadow ease-in-out 0.15s;
    transition: border-color ease-in-out 0.15s, box-shadow ease-in-out 0.15s;
}
.lan{
    background:#382451;border-top:1px solid #271838;line-height:30px;padding-left:15px;border-bottom:1px solid #845ab6;border-radius:15px;height:30px;width:90px;margin-top:12px;
}
.lan a:hover{
    color:#e3d9ee;
}
.banner_shadow{
    background:url(<%= asset_path 'banner_shadow.png'%>) no-repeat center top;
