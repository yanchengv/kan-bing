/*--------------/imgscroll-----------*/
a { color:#000; font-size:9pt; text-decoration:none; }
a:hover { color:#0066cc; }
.blk_18 { overflow:hidden; zoom:1; position:relative;font-size:9pt;border-radius:8px;width:100%; padding-left:10px;background:#eef1f9;border:1px solid #e1eaff; }
.blk_18 .pcont { width:88%; float:left; overflow:hidden; margin-left:40px;}
.blk_18 .ScrCont { width:32766px; zoom:1; margin-left:-5px; }
.blk_18 #List1_1, .blk_18 #List2_1 { float:left; }
.blk_18 .LeftBotton, .blk_18 .RightBotton { width:15px; height:86px; float:left;}
.blk_18 .LeftBotton { font-size:20px;color:#828486;font-weight:bold;padding-top:4px;padding-left:15px;line-height:75px;position:absolute;left:0px; bottom:25px;width:46px;height:86px;}
.blk_18 .RightBotton {padding-left:10px;font-size:20px;padding-top:4px;font-weight:bold;color:#828486;line-height:75px;position:absolute;right:0px; bottom:25px;width:46px;height:86px;}
.blk_18 .LeftBotton:hover { color:#467bdd;}
.blk_18 .RightBotton:hover {color:#467bdd;}
.blk_18 .pl img {  display:block; cursor:pointer; border:none; margin:6px auto 6px auto;box-shadow:1px 2px 3px rgba(67,86,120,.5)}
.blk_18 .pl { width:110px; border:1px solid transparent; float:left; float:left; text-align:center; line-height:24px; }

