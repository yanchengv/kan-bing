    @font-face {font-family: "iconfont";
        src: url('<%=asset_path "weixins/font/iconfont.eot"%>'); /* IE9*/
        src: url('<%=asset_path "weixins/font/iconfont.eot?#iefix"%>') format('embedded-opentype'), /* IE6-IE8 */
        url('<%=asset_path "weixins/font/iconfont.woff"%>') format('woff'), /* chrome、firefox */
        url('<%=asset_path "weixins/font/iconfont.ttf"%>') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
        url('<%=asset_path "weixins/font/iconfont.svg#iconfont"%>') format('svg'); /* iOS 4.1- */
    }

    .iconfont {
        font-family:"iconfont" !important;
        font-size:16px;
        font-style:normal;
        -webkit-font-smoothing: antialiased;
        -webkit-text-stroke-width: 0.2px;
        -moz-osx-font-smoothing: grayscale;
    }
    .icon-weibiaoti1:before { content: "\e601"; }
    .icon-nuclear:before { content: "\e602"; }
    .icon-report:before { content: "\e603"; }
    .icon-ct:before { content: "\e606"; }
    .ct_img_nums{
        background:rgba(1,160,226,.8);
        position:absolute;
        left:50%;
        margin-left:-20%;
        height:70px;
        width:40%;
        color:#fff;
        top:50%;
        margin-top:-35px;
        font-size:16px;
        font-family:"Microsoft YaHei",Arial;
        z-index:1;
       text-shadow:none;
        text-align:center;
        padding-top:5px;
    }
    .ct_img_nums p span{
        font-size:26px;
    }

    .ct_img a{
        position:relative;
        display:block;
        width:98%;
        margin:15px auto;
        z-index:0;
    }
    .ct_img img{
        width:100%;
        border-radius:4px;
        box-shadow:0px 1px 1px rgba(0,0,0,.3)
    }
    .ct_img_list{
        width:100%;
        margin:0;
        padding:0;
        list-style:none;
    }
    .ct_img_list li{

        list-style:none;
    }
    .ct_img_list li img{
        width:98%;
        border-radius:4px;
        box-shadow:0px 1px 1px rgba(0,0,0,.3);
        margin:10px auto;
    }
    @media \0screen\,screen\9 {/* 只支持IE6、7、8 */
        .ct_img_nums{
            background-color:#01A0E2;
            filter:Alpha(opacity=80);
            position:static;
            *zoom:1;
        }
        .ct_img_nums p{
            position: relative;
        }
    }