<tr id="cons_<%=item.id%>" >
  <td style="color:#6b6b6d"><%= item.name %></td>
  <td style="color:#6b6b6d"><%= item.owner.name %></td>
  <td style="color:#6b6b6d"><%= item.format_schedule_time_for_print %></td>
  <td style="color:#6b6b6d">
    <% if item.join?(current_user.doctor_id)%>
        已确认参加
    <% else %>
        <%= link_to("确认参加", consultation_path('join/'+item.id.to_s),:style =>"color:#6b6b6d") %>
    <% end %>
  </td>
</tr>