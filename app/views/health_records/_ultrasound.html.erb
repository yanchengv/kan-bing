<%= javascript_include_tag 'paginate' %>
<br>
<div class="panel panel-default">
  <div class="panel-heading"><%= session["name"] %></div>
  <table class="table">
    <tr>
      <th>医院</th>
      <th>科室</th>
      <th>检查医生</th>
      <th>检查类别</th>
      <th>检查日期</th>
      <th>查看</th>
    </tr>
    <% @irs.each do |r| %>
        <tr>
          <td><%= r.hospital %></td>
          <td><%= r.department %></td>
          <td><%= r.doctor %></td>
          <td><%= r.child_type %></td>
          <td><%= r.checked_at %></td>
          <td><a href="http://www.baidu.com" target="_blank">查看</a></td>
        </tr>
    <% end %>
  </table>
  <%= will_paginate @irs,:previous_label => '<', :next_label => '>',:class=>'pagination-mini page num unstyled',:style=>'margin-left:300px;margin-bottom:40px;' %>
</div>
