    .hos_info_box {
        border-top: 1px solid #dcdcdc;
        border-bottom:1px solid transparent;
        border-left:1px solid transparent;
        border-right:1px solid transparent;
        padding:10px;
    }

    .hos_info_box:hover {
        border: 1px solid #29a5a2;
    }

    .hos_info p {
        font-family: "Microsoft YaHei", Arial;
        color: #999;
        font-size:12px;
    }

    .hos_info {
        padding-top: 10px;
    }
        .jy_num{
            font-size: 18px;
            color: #5ca945;
            line-height: 22px;
            font-family: "Microsoft YaHei", Arial;
        }
        .hos_con{
