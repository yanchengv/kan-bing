        font-family: "Microsoft YaHei", Arial;
        border-right: 1px solid #dcdcdc;
        border-bottom: 1px solid #dcdcec;
    }

    .hos_table td a {
        font-family: "Microsoft YaHei", Arial;
        color: #27bab4;
        margin: 6px 12px;
        padding: 2px 6px;
        display: inline-block;
    }

    .hos_table td a:hover {
        background: rgb(218, 249, 249);
    }

    .hos_table_t {
        background: #f5f5f5;
        width: 110px;
        vertical-align: top;
        text-align: center;
        margin: 8px 0;
        color: #888;
    }

    .hos_bg {
        font-family: "Microsoft YaHei", Arial;
        background: #f5f5f5;
        padding: 10px;
